doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Document
    link(rel="stylesheet", href="https://unpkg.com/swiper/swiper-bundle.min.css")
    link(rel="stylesheet", href="/style.css")
    link(rel="stylesheet", href="/icon/css/icon.css")
  body
    include header-main-page
    include form-modal
    
     
    
    .hero
      .hero__video-bg
        video(autoplay loop muted="true" preload="auto")
          source(src='/img/Muras.mp4' type='video/mp4')
      .hero__text
        .container 
          .hero__title Квартиры <br> в рассрочку <br> под 0%
          .hero__subtitle Недвижимость по выгодным <br> ценам в жилом доме «Мурас»
          a(href="#").btn.consultation-js 
            span Узнать больше 
    .banner 
      .container 
        .banner__wrap
          a(href="#").banner__item.consultation-js
            .banner__title Квартиры в рассрочку <br> без % и переплат
            .banner__img
              img(src="/img/banner1.png", alt="")
          a(href="#").banner__item.consultation-js
            .banner__title Бартер на авто <br> и недвижимость
            .banner__img
              img(src="/img/banner2.png", alt="")
    
    .object
      .container
        .title-wrap
          h2.title Наши объекты
          .select.filter-select
            .selected Все объекты
            .select-dropdown
              .select-option.all-objects-js(data-value="Все объекты") Все объекты
              .select-option.building-objects-js(data-value="Строящиеся") Строящиеся
              .select-option.done-objects-js(data-value="Завершенные") Завершенные
          a(href="#").link Показать на карте   
            
        //- Все объекты  
        .object__type.all-objects
          .swiper.Swiper1
            .swiper-wrapper
              mixin allObjects(img,name,address,type,floor,link,status)
                .swiper-slide
                  .object__item
                    .hidden-link(data-value=link hidden target="_blank")
                    img(src="/img/"+img, alt="")
                    if status === "none"
                    else 
                      .object__status.btn=status
                    .object__info
                      .object__info-top
                        .object__name=name
                      .object__info-bottom
                        .object__info-bottom-item=address
                        .object__info-bottom-item=type
                        .object__info-bottom-item=floor
                
                
              +allObjects('Мурас.png', 'Жилой дом «Мурас»', 'Курчатова 25, «Нижний Джал»', 'Комфорт класс','10 этажей', '#1',  'none')
              +allObjects('Жетиген2.png', 'Жилой дом «Жетиген»', 'Муса-Джалила 132, пер. Буденого', 'Комфорт класс','8 этажей', '#2',  'none')
            .swiper-button-next
            .swiper-button-prev

        //- Строящиеся объекты
        .object__type.building-objects
          .swiper.Swiper2
            .swiper-wrapper
              +allObjects('object', ' Жилой дом «Мурас»', 'Курчатова 25, «Нижний Джал»', 'Комфорт класс','10 этажей', '#3', 'На стадии строительтва')
              +allObjects('object', ' Жилой дом «Мурас»', 'Курчатова 25, «Нижний Джал»', 'Комфорт класс','10 этажей', '#4', 'На стадии строительтва')
            .swiper-button-next
            .swiper-button-prev

        //- Завершенные объекты
        .object__type.done-objects
          .swiper.Swiper3
            .swiper-wrapper
              +allObjects('object', ' Жилой дом «Мурас»', 'Курчатова 25, «Нижний Джал»', 'Комфорт класс','10 этажей', '#5', 'Строительтво завершено')
              +allObjects('object', ' Жилой дом «Мурас»', 'Курчатова 25, «Нижний Джал»', 'Комфорт класс','10 этажей', '#6', 'Строительтво завершено')
            .swiper-button-next
            .swiper-button-prev

    .apartment
      .container
        h2.title Ваша квартира мечты
        .wrap
          mixin item(img,title) 
            a(href="#").col 
              .img 
                img(src="/img/col"+img+".jpg", alt="") 
              .title!=title 
          +item(1,'1 комнатные квартиры')
          +item(2,'2 комнатные квартиры')
          +item(3,'3 комнатные квартиры')
    
    .advantages 
      .container 
        .advantages__banner 
          img(src="/img/advantage_banner.png", alt="")          
          .advantages__banner-title Качество <br> превыше <br> всего 
        .wrap
          mixin item(img,title) 
            a(href="#").col 
              .img.img--tranlste-off
                img(src="/img/col"+img+".png", alt="") 
              .title!=title 
          +item(4, 'Более 11 лет опыта <br> в сфере строительства')
          +item(5, 'Высококвалифицированный <br> штат сотрудников')
          +item(6, 'Имеем все документы для <br> производства строительных работ')

    include form 
    

    .documentation 
      .container
        .title-wrap
          h2.title Наша документация
          .select.filter-select
            .selected Все
            .select-dropdown
              .select-option.all-ducumentation-js(data-value="Все") Все 
              .select-option.objects-js(data-value="Объекты") Объекты
              .select-option.company-js(data-value="Компания") Компания

        .documentation__type.all-ducumentation 
          .swiper.Swiper4
            .swiper-wrapper
              mixin documentation(name, link)
                .swiper-slide
                  .documentation__item
                    .documentation__item-name!=name
                    a(href=link download).documentation__item-link Скачать PDF файл
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
            .swiper-button-next
            .swiper-button-prev
        .documentation__type.objects
          .swiper.Swiper5
            .swiper-wrapper
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
            .swiper-button-next
            .swiper-button-prev  

        .documentation__type.company
          .swiper.Swiper6
            .swiper-wrapper
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
              +documentation('Лицензия <br> на строительтво','#')
              +documentation('Градостроительное <br> заключение','#')
            .swiper-button-next
            .swiper-button-prev
    
    .news 
      .container
        #instafeed 


    include footer 

       
    
    
    script(src="https://code.jquery.com/jquery-3.6.0.js")  
    script(src="https://unpkg.com/swiper/swiper-bundle.min.js")  
    script(src="/script.js")