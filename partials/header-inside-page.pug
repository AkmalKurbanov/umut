header.header
  .container
    .header__col-wrap
      .header__col
        .header__logo
          a(href="#")
            img(src="/img/logo.png" alt="logo") 
      .header__col
        ul.nav
          mixin nav(item)
            li
              a(href="#")=item
          +nav('Объекты')
          +nav('Акции')
          +nav('Новости')
          +nav('О нас')
          +nav('Контакты')
        .header__phones
          mixin phone(phone)
            a(href="tel:"+phone  class="header__phone")=phone 
          +phone('+996 501 44 22 22')
          +phone('+996 221 44 22 22')
    